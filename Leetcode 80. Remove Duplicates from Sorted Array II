class Solution {
public:
    int removeDuplicates(vector<int>& nums) {
        
        map<int , int>mp;

        for(auto i : nums){
            mp[i]++;
        }

        int c = 0;


        for(auto i : mp){
            if(i.second >= 2){
                nums[c++] = i.first;
                nums[c++] = i.first;
            }else{
                nums[c++] = i.first;
            }
        }
        return c;
    }
};
